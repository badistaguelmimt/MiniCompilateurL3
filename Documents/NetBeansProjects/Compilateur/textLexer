# Exemple de code pour tester le lexer

x = 10
y = 3.5
message = "Bonjour monde"

if x > y:
    print("x est plus grand")
    while x > 0:
        x = x - 1
        # boucle

else:
    print('y est plus grand')
    for i in range(3):
        pass

def calcul(a, b):
    if a == b:
        return True
    elif a < b:
        return False
    else:
        return None

try:
    resultat = calcul(x, y)
except Exception as e:
    raise e
finally:
    print("TerminÃ©")

